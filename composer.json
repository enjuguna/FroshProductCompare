{
    "name": "kware/product-compare",
    "description": "WhatsApp plugin for Shopware 6",
    "version": "1.4.1",
    "type": "shopware-platform-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "FriendsOfShopware",
            "email": "info@friendsofshopware.com"
        },
        {
            "name": "Thuong Le",
            "email": "levienthuong@gmail.com"
        }
    ],
    "require": {
        "shopware/core": "^6.4",
        "shopware/storefront": "^6.4"
    },
    "extra": {
        "shopware-plugin-class": "Kware\\KwareProductCompare\\KwareProductCompare",
        "plugin-icon": "src/Resources/config/plugin.png",
        "copyright": "(c) by Justa Vin<levienthuong@gmail.com>",
        "label": {
            "de-DE": "Ein einfaches Produktvergleichs-Plugin für Shopware 6",
            "en-GB": "A WhatsApp plugin for Shopware 6"
        },
        "description": {
            "de-DE": "Kostenloses Open Source Plugin für Shopware 6. Ermöglicht es bis zu 4 Produkte miteinander zu vergleichen. Vergleichsliste auf der Produktvergleichsseite oder auf der Artikelseite.",
            "en-GB": "Shopware 6 order via WhatApp plugin."
        },
        "manufacturerLink": {
            "de-DE": "https://github.com/FriendsOfShopware/KwareProductCompare",
            "en-GB": "https://github.com/FriendsOfShopware/KwareProductCompare"
        },
        "supportLink": {
            "de-DE": "https://github.com/FriendsOfShopware/KwareProductCompare/issues",
            "en-GB": "https://github.com/FriendsOfShopware/KwareProductCompare/issues"
        }
    },
    "autoload": {
        "psr-4": {
            "Kware\\KwareProductCompare\\": "src/"
        }
    },
    "conflict": {
        "shopware/storefront": "<6,>=7",
        "shopware/administration": "<6,>=7"
    },
    "prefer-stable": true
}
